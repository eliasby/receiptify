{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "singleQuote": true,
  "semi": false,
  "quoteProps": "preserve",
  "importOrder": [
    "@testing-library/react",
    "^react$",
    "^react-dom/(.*)$",
    "^react-router-dom$",
    "^react-redux$",
    "^antd$",
    "^@ant-design/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^[./](.*)$"
  ],
  "importOrderSeparation": false
}
